{
  "name": "@sbf/n8n-nodes-sbf",
  "version": "0.1.0",
  "description": "n8n node for Second Brain Foundation integration",
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lintfix": "tslint --fix -p tsconfig.json -c tslint.json",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.js nodes"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/SbfApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/SBF/SBF.node.js",
      "dist/nodes/SBFTrigger/SBFTrigger.node.js"
    ]
  },
  "keywords": [
    "n8n",
    "n8n-nodes",
    "sbf",
    "second-brain",
    "automation",
    "va"
  ],
  "author": "SBF Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.0.0",
    "n8n-core": "^1.0.0"
  },
  "dependencies": {}
}
