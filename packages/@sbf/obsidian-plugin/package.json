    "lint": "eslint src",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "obsidian-plugin",
    "second-brain",
    "knowledge-management"
  ],
  "author": "SBF Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.19.0",
    "obsidian": "latest",
    "typescript": "^5.3.0"
  },
  "dependencies": {}
}
