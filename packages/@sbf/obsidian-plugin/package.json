{
  "name": "@sbf/obsidian-plugin",
  "version": "1.0.0",
  "private": true,
  "description": "Second Brain Foundation Companion Plugin for Obsidian",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
