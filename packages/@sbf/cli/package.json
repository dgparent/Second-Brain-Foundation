{
  "name": "@sbf/cli",
  "version": "1.0.0",
  "description": "Second Brain Foundation Command Line Interface",
  "type": "module",
  "bin": {
    "sbf": "./dist/bin/sbf.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/bin/sbf.js",
    "test": "jest",
    "lint": "eslint src",
    "clean": "rimraf dist"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "sbf",
    "second-brain",
    "knowledge-management",
    "cli",
    "ai"
  ],
  "author": "SBF Team",
  "license": "MIT",
  "dependencies": {
    "@inquirer/prompts": "^7.0.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "glob": "^10.4.0",
    "gray-matter": "^4.0.3",
    "ora": "^8.0.0",
    "undici": "^6.19.0",
    "unzipper": "^0.12.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/unzipper": "^0.10.9",
    "rimraf": "^5.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
